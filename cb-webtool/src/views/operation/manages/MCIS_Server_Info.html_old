{{define "Mcis_server_info"}}
<!-- detail -->
					<div class="dashboard dashboard_cont dashboard_detail server_info" id="dashboard_detailBox">
						
						<div class="d_box box_m box_w100">
							<div class="titbox">
								<div class="tit initial"><strong>Server Info</strong> <span class="stxt" id="server_info_text">[ Server 2 / mcis-t03 ]</span></div>
								<div class="top_info">
									<ul>
										<li>
											<a href="javascript:void(0);">Analytics View</a>
											<div class="infobox">
												<div class="box">
													<p>Server</p>
													<ul>
														<li><a href="javascript:void(0);">Performance</a></li>
														<li><a href="javascript:void(0);">Fault</a></li>
														<li><a href="javascript:void(0);">Cost</a></li>
														<li><a href="javascript:void(0);">Utilize</a></li>
													</ul>
												</div>
											</div>
										</li>
										<li><a href="javascript:void(0);"><img src="/assets/img/common/icon_question.png" alt=""/></a></li>
									</ul>
								</div>
							</div>
							
							<div class="ds_cont">
								
								<div class="iparea">
									<div class="boxlist ipbox d_ip1">
										<div class="iconbox">
											<div class="img" id="server_info_status_img"><img src="/assets/img/contents/icon_running_db.png" alt=""/></div>
											<!-- 
											<div class="img"><img src="/assets/img/contents/icon_stop_db.png" alt=""/></div>
											<div class="img"><img src="/assets/img/contents/icon_terminate_db.png" alt=""/></div>
											 -->
										</div>
										<ul>
											<li>
												<label>Name</label>
												<input type="text" name="" value="" placeholder="Name / ID" title="" readonly id="server_info_name"/>
											</li>
											<li>
												<label>Description</label>
												<input type="text" name="" value="" placeholder="description" title="description" readonly id="server_info_desc"/>
												<!-- 
												<label class="ta">Description</label>
												<textarea  cols="" rows="" placeholder="description" title="" class="ta_line" readonly ></textarea> 
												-->
											</li>
										</ul>
									</div>
									
									<div class="boxlist ipbox d_ip2">
										<ul>
											<li>
												<label>Public IP</label>
												<input type="text" name="" value="" placeholder="Public IP" title="" id="server_info_public_ip" readonly />
											</li>
											<li>
												<label>/ DNS</label>
												<input type="text" name="" value="" placeholder="Public DNS" title="" readonly id="server_info_public_dns"/>
											</li>
											<li>
												<label>Private IP</label>
												<input type="text" name="" value="" placeholder="Private IP" title="" readonly id="server_info_private_ip"/>
											</li>
											<li>
												<label>/ DNS</label>
												<input type="text" name="" value="" placeholder="Private DNS" title="" readonly id="server_info_private_dns"/>
											</li>
										</ul>
									</div>
									
									<div class="boxlist ipbox d_ip3">
										<ul>
											<li>
												<label>Operating System</label>
												<input type="text" name="" value="" placeholder="Operating System" title="" readonly id="server_info_os"/>
											</li>
											<li>
												<label>Architecture</label>
												<input type="text" name="" value="" placeholder="Architecture" title="" readonly id="server_info_archi"/>
											</li>
											<li>
												<label>VMSpecName</label>
												<input type="text" name="" value="t2.micro" placeholder="" title="" readonly id="server_info_vmspec_name"/>
											</li>
											<li>
												<label>StartTime</label>
												<input type="text" name="" value="2020-05-08T14:49:47Z" placeholder="" title="" readonly id="server_info_start_time"/>
											</li>
										</ul>
									</div>
									
									<div class="boxlist ipbox d_ip4">
										<div class="iconbox">
											<div class="img" id="server_info_csp_icon"><img src="/assets/img/contents/img_logo_a.png" alt=""/></div>
										</div>
										<ul>
											<li>
												<label>Region</label>
												<input type="text" name="" value="Seoul : 리전(지역)" placeholder="" title="" readonly id="server_info_region"/>
											</li>
											<li>
												<label>Zone</label>
												<input type="text" name="" value="a1 : 존" placeholder="" title="" readonly id="server_info_zone"/>
											</li>
											<li>
												<label>VMID</label>
												<input type="text" name="" value="cspVMID : CSP VM ID" placeholder="" title="" readonly id="server_info_cspVMID"/>
											</li>
											<li>
												<label>ConfigName</label>
												<input type="text" name="" value="" placeholder="Cloud Connection name" title="" readonly id="server_info_connection_name"/>
											</li>
										</ul>
									</div>
								</div>
							</div>
							
							
							<!-- detail Info -->
							{{template "Mcis_detail_info" .}}
							<!-- //detail Info -->
							
														
						</div>
						
					</div>
					<!-- //detail -->
{{end}}